1-SELECT rating, COUNT(*) as film_count FROM film GROUP BY rating;  
2-SELECT replacement_cost, COUNT(*) as film_count FROM film GROUP BY replacement_cost HAVING COUNT(*) > 50 ORDER BY film_count DESC;  
3-SELECT store_id, COUNT(*) as customer_count FROM customer GROUP BY store_id;  
4-SELECT country_id, COUNT(*) as city_count FROM city GROUP BY country_id ORDER BY city_count DESC LIMIT 1;  
